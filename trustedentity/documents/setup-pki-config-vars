# Symbols that are hardcoded
PKI_CA=pki/ca
PKI_CRL=pki/crl
PKI_CERT=pki/cert
PKI_ROLES=pki/roles
PKI_ISSUE=pki/issue

PKI_CA_PEM=$PKI_CA/pem
PKI_CRL_PEM=$PKI_CRL/pem
PKI_CERT_CA=$PKI_CERT/ca
PKI_CERT_CRL=$PKI_CERT/crl
